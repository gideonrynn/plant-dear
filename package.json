{
  "name": "plant-dear",
  "version": "1.0.0",
  "description": "plant care and health planner",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "seed": "node scripts/seed.js"
  },
  "dependencies": {
    "ajax": "0.0.4",
    "axios": "^0.21.4",
    "dotenv": "^8.6.0",
    "express": "^4.17.3",
    "if-env": "^1.0.4",
    "moment": "^2.29.1",
    "mongoose": "^6.2.3",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gideonrynn/plant-dear.git"
  },
  "keywords": [
    "plant",
    "care",
    "plant",
    "health"
  ],
  "author": "sarah dillard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gideonrynn/plant-dear/issues"
  },
  "homepage": "https://github.com/gideonrynn/plant-dear#readme"
}
